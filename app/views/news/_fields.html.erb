<%= render 'shared/error_messages', object: f.object %>

<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :news_body %>
  <%= f.text_area :news_body, rows: 5, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :news_image %>
  <%= f.file_field :news_image %>
</div>
